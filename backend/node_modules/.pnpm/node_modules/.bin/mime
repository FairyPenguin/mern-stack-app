#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/mime@1.6.0/node_modules:/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/mime@1.6.0/node_modules:/media/mamhoud/7ec5d104-ab7f-45f1-a837-a0a7718a2361/Python/node🐛/mern-stack/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
